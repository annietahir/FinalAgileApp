<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="agilegroup6.com.agilescanapp">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudentScanRequest"
            android:label="@string/title_activity_student_scan_request"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StaffMenu"
            android:label="@string/title_activity_staff_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GenerateQRCode"
            android:label="@string/title_activity_generate_qrcode"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
